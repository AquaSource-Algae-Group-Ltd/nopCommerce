@inject Nop.Core.Domain.Common.CommonSettings commonSettings
@inject IThemeContext themeContext
@{
    var themeName = await themeContext.GetWorkingThemeNameAsync();
}
<!DOCTYPE html>
<html lang="@CultureInfo.CurrentUICulture.TwoLetterISOLanguageName" class="@pageHeadBuilder.GeneratePageCssClasses()">
<head>
    <title>@pageHeadBuilder.GenerateTitle()</title>
    <meta http-equiv="Content-type" content="text/html;charset=UTF-8" />
    <link href="@Url.Content($"~/Themes/{themeName}/Content/css/print.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/lib_npm/jquery/jquery.min.js")"></script>
    @*/when jQuery migrate script logging is active you will see the log in the browser console*@
    @if (commonSettings.JqueryMigrateScriptLoggingActive)
    {
        <script src="@Url.Content("~/lib_npm/jquery-migrate/jquery-migrate.js")"></script>
    }
    else
    {
        <script src="@Url.Content("~/lib_npm/jquery-migrate/jquery-migrate.min.js")"></script>
    }
    @pageHeadBuilder.GenerateInlineScripts(ResourceLocation.Head)
</head>
<body>
    @RenderBody()
    @pageHeadBuilder.GenerateInlineScripts(ResourceLocation.Footer)
</body>
</html>
